{
    "version": "0.0.1",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "repository": {
        "url": "https://github.com/pekerleke00/ts-printer",
        "type": "git"
    },
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "ts-printer",
    "author": "pekerleke00",
    "module": "dist/ts-printer.esm.js",
    "size-limit": [
        {
            "path": "dist/ts-printer.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/ts-printer.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.1.0",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "husky": "^8.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "size-limit": "^8.1.0",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.1",
        "typescript": "^3.9.10"
    },
    "keywords": [
        "typescript",
        "log",
        "console",
        "print"
    ]
}